        "# Function to save weather data to a JSON file\n",

        "def save_weather_data(data, filename):\n",

        "    # Open the file in write mode and save the data as JSON\n",

        "    with open(filename, 'w') as file:\n",

        "        json.dump(data, file)\n",

        "\n",

        "# Function to load weather data from a JSON file\n",

        "def load_weather_data(filename):\n",

        "    # Open the file in read mode and load the data from JSON\n",

        "    with open(filename, 'r') as file:\n",

        "        data = json.load(file)\n",

        "    return data\n",

        "\n"

      ]

